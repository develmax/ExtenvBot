@model SubscriptionsViewModel
@{
    ViewData["Title"] = "Telegram Subscribes";
}

<div class="row">
    <table class="table">
        <tr>
            <td>ChatId</td>
            <td>Name</td>
            <td>Envs</td>
        </tr>
        
        @for (int i = 0; i < Model.EntityList.Count; i++)
        {
            <tr>
                <td>@Model.EntityList[i].ChatId</td>
                <td>@Model.EntityList[i].Name</td>
                <td>@{var a = (Model.EntityList[i].Envs ?? string.Empty).Replace(",", ", ");}@a</td>
            </tr>
        }
    </table>
</div>
